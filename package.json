{
    "name": "yuyan-front",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build:spa": "vue-cli-service build",
        "lint": "vue-cli-service lint --fix",
        "build:client": "cross-env WEBPACK_TARGET=web vue-cli-service build",
        "build:server": "cross-env WEBPACK_TARGET=node vue-cli-service build --no-clean",
        "build": "cross-env NODE_NEV=production npm run build:client && npm run build:server",
        "dev": "concurrently \"npm run serve\" \"npm run dev:serve\"",
        "dev:serve": "nodemon ./server/index.js"
    },
    "dependencies": {
        "ant-design-vue": "~1.6.5",
        "axios": "^0.20.0",
        "core-js": "^3.6.5",
        "vue": "~2.6.11",
        "vue-class-component": "^7.2.3",
        "vue-property-decorator": "^8.4.2",
        "vue-router": "~3.2.0",
        "vuex": "~3.4.0",
        "vuex-router-sync": "^5.0.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^2.33.0",
        "@typescript-eslint/parser": "^2.33.0",
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-plugin-router": "~4.5.0",
        "@vue/cli-plugin-typescript": "~4.5.0",
        "@vue/cli-plugin-vuex": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/eslint-config-typescript": "^5.0.2",
        "babel-plugin-import": "^1.13.0",
        "concurrently": "^5.3.0",
        "cross-env": "^7.0.2",
        "eslint": "^6.7.2",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-vue": "^6.2.2",
        "husky": "^4.3.0",
        "koa": "^2.13.0",
        "koa-router": "7.4.0",
        "koa-static": "^5.0.0",
        "less": "^3.12.2",
        "less-loader": "5.0.0",
        "less-vars-to-js": "^1.3.0",
        "lint-staged": "^10.4.0",
        "lodash.merge": "^4.6.2",
        "nodemon": "^2.0.4",
        "prettier": "^1.19.1",
        "stylelint": "^13.7.1",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-order": "^4.1.0",
        "typescript": "~3.9.3",
        "vue-server-renderer": "^2.6.12",
        "vue-template-compiler": "^2.6.11",
        "webpack": "^4.44.2",
        "webpack-node-externals": "^2.5.2"
    }
}
