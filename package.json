{
    "name": "yuyan-front",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build:client": "vue-cli-service build",
        "build:server": "cross-env WEBPACK_TARGET=node vue-cli-service build --mode server",
        "build:win": "cross-env NODE_ENV=production npm run build:server  && move dist\\vue-ssr-server-bundle.json bundle && npm run build:client && move bundle dist\\vue-ssr-server-bundle.json",
        "build": "cross-env NODE_ENV=production  npm run build:server && mv ./dist/vue-ssr-server-bundle.json ./bundle && npm run build:client && mv ./bundle ./dist/vue-ssr-server-bundle.json",
        "dev": "concurrently \"npm run serve\" \"npm run dev:serve\"",
        "dev:serve": "cross-env WEBPACK_TARGET=node nodemon ./server/index.js",
        "lint": "vue-cli-service lint --fix"
    },
    "dependencies": {
        "ant-design-vue": "~1.6.5",
        "axios": "^0.20.0",
        "core-js": "^3.6.5",
        "vue": "~2.6.11",
        "vue-class-component": "^7.2.3",
        "vue-property-decorator": "^8.4.2",
        "vue-router": "~3.2.0",
        "vuex": "~3.4.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^2.33.0",
        "@typescript-eslint/parser": "^2.33.0",
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-plugin-router": "~4.5.0",
        "@vue/cli-plugin-typescript": "~4.5.0",
        "@vue/cli-plugin-vuex": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/eslint-config-typescript": "^5.0.2",
        "babel-plugin-import": "^1.13.0",
        "concurrently": "^5.3.0",
        "cross-env": "^7.0.2",
        "eslint": "^6.7.2",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-vue": "^6.2.2",
        "husky": "^4.3.0",
        "lint-staged": "^10.4.0",
        "lodash.merge": "^4.6.2",
        "nodemon": "^2.0.4",
        "prettier": "^1.19.1",
        "stylelint": "^13.7.1",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-order": "^4.1.0",
        "typescript": "~3.9.3",
        "vue-server-renderer": "^2.6.12",
        "vue-template-compiler": "^2.6.11"
    }
}
